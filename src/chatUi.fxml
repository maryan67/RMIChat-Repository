<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<VBox prefHeight="400.0" prefWidth="640.0" xmlns="http://javafx.com/javafx/9.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.client.chatUiController">
  <children>
    <AnchorPane maxHeight="-1.0" maxWidth="-1.0" prefHeight="-1.0" prefWidth="-1.0" VBox.vgrow="ALWAYS">
      <children>
            <Button fx:id="disconnectButton" layoutX="524.0" layoutY="23.0" mnemonicParsing="false" onAction="#onDisconnectClick" text="Disconnect" />
            <Button fx:id="connectButton" layoutX="440.0" layoutY="23.0" mnemonicParsing="false" onAction="#onConnectClick" text="Connect" />
            <Button fx:id="sendMessageButton" layoutX="578.0" layoutY="361.0" mnemonicParsing="false" onAction="#onSendClick" text="Send" />
            <TextField fx:id="messageText" layoutX="22.0" layoutY="361.0" prefHeight="25.0" prefWidth="511.0" />
            <ListView fx:id="onlineUsers" layoutX="424.0" layoutY="148.0" prefHeight="196.0" prefWidth="200.0" />
            <Label fx:id="chatLabel" layoutX="22.0" layoutY="15.0" prefHeight="326.0" prefWidth="396.0" text="Start by typing your name and press connect...." />
            <TextField fx:id="userNameText" layoutX="424.0" layoutY="79.0" prefHeight="25.0" prefWidth="200.0" />
            <Label layoutX="426.0" layoutY="62.0" text="Name:" />
            <Button fx:id="selectParticipantButton" layoutX="498.0" layoutY="123.0" mnemonicParsing="false" onAction="#onSelectClick" text="Select" />
      </children>
    </AnchorPane>
  </children>
</VBox>
